    }
  },
  "host": "tobefilledin.whenknown.com",
  "basePath": "/endpoing",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/people": {
      "get": {
        "description": "Returns all people that are registerd to use the system.",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A list of people.",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Person"
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Person": {
      "type": "object",
      "required": [
        "username"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "username": {
          "type": "string"
        },
        "passwrd": {
          "type": "string"
        },
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        }
      }
    }
  }
}